{
  "name": "small-valery",
  "version": "0.0.1",
  "license": "ISC",
  "scripts": {
    "sleep": "sleep 3",

    "clean": "rimraf -rf ./dist",
    "predev": "npm run clean",
    "prebuild": "npm run clean",

    "json-server": "babel-node ./node_modules/.bin/json-server --watch -c ./config/json-server/config.json ./config/json-server/testdata.babel.js",

    "watch": "npm run webpack -- -w",
    "webpack": "NODE_ENV=development webpack --config=./config/webpack/dev.babel.js -c --process",
    "dev": "npm-run-all -p watch json-server",

    "open-dev": "open-url localhost:8000",
    "open-dev-url": "npm-run-all sleep open-dev",
    "webpack-dev-server": "NODE_ENV=development webpack-dev-server -w -d --config=./config/webpack/dev.babel.js --content-base=./src",
    "json-server.start": "npm-run-all -p webpack-dev-server json-server open-dev-url",
    "start": "npm-run-all -p webpack-dev-server open-dev-url",

    "open-prod": "open-url localhost:8080",
    "open-prod-url": "npm-run-all sleep open-prod",
    "start-server-and-open-prod-url": "npm-run-all -p open-prod-url json-server",
    "no-json-server.start-server-and-open-prod-url": "npm-run-all -p open-prod-url",
    "build": "NODE_ENV=production webpack --config=./config/webpack/prod.babel.js -c --progress --profile --bail -p",
    "serve": "npm-run-all build start-server-and-open-prod-url",

    "lint": "tslint 'src/**/*.ts'",

    "disabled.test": "karma start ./config/karma.config.js --single-run",
    "disabled.tdd": "karma start ./config/karma.config.js",

    "disabled.e2e": "npm-run-all -p prepare-e2e wait-20s-and-run-protractor",
    "pree2e": "webdriver-manager update",
    "prepare-e2e": "concurrently -p '[{name}]' -n 'HTTP' -c 'bgYellow.bold' 'npm run webpack-dev-server' --kill-others --success first",
    "wait-20s-and-run-protractor": "concurrently -p '[{name}]' -n 'PROTRACTOR' -c 'bgGreen.bold' 'npm-run-all wait-20s protractor' --kill-others --success first",
    "postwait-20s-and-run-protractor": "npm run stop",
    "wait-20s": "sleep 20",
    "protractor": "protractor ./config/protractor.js",
    "stop": "kill -9 $(ps waux | grep webpack-dev-server | grep -v color | awk '{print $2}') | true"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.1.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "react-css-transition-replace": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.16.3",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.8.0",
    "editorconfig": "^0.13.2",
    "eslint": "^3.8.1",
    "eslint-config-stylelint": "^5.0.0",
    "eslint-config-xo": "^0.17.0",
    "eslint-plugin-react": "^6.4.1",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.1.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "husky": "^0.11.9",
    "imports-loader": "^0.6.5",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.7.0",
    "json-loader": "^0.5.4",
    "json-server": "^0.8.22",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.0",
    "npm-run-all": "^3.1.1",
    "open-url": "^2.0.2",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss-loader": "^1.0.0",
    "protractor": "^4.0.9",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "to-string-loader": "^1.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.16.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.15.0"
  },
  "babel": {
    "presets": [
      "stage-0",
      "react",
      "es2015"
    ],
    "plugins": [
      "react-html-attrs",
      "add-module-exports",
      "transform-class-properties"
    ]
  },
  "description": "small-valery",
  "main": "src/main.jsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/small-valery.github.com.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Maksim Kostromin",
  "bugs": {
    "url": "https://github.com/daggerok/small-valery.github.com/issues"
  },
  "homepage": "https://github.com/daggerok/small-valery.github.com#readme"
}
